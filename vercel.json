{
  "version": 2,
  "routes": [
    {
       "src": "/api/(.*)",
       "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
